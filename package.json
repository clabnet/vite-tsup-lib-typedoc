{
  "name": "vite-tsup-lib-typedoc",
  "version": "0.1.0",
  "description": "Build library with vite, tsup and typedoc",
  "license": "MIT",
  "author": {
    "name": "Claudio Barca",
    "email": "c.barca@libero.it",
    "url": "https://github.com/clabnet"
  },
  "contributors": [
    {
      "name": "Brian Ng",
      "email": "brianngchongeu@gmail.com",
      "url": "https://github.com/brifiction"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "keywords": [
    "vite",
    "tsup",
    "template",
    "boilerplate",
    "node",
    "module",
    "library",
    "typedoc"
  ],
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "typings": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "doc": "typedoc",
    "build": "tsup && pnpm doc",
    "build:watch": "tsup --watch",
    "dev": "vite",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "npm-pack": "pnpm build && cd ./dist && npm pack",
    "npm-publish": "pnpm build && cd ./dist && pnpm publish --access public --loglevel silly"
  },
  "dependencies": {
    "luxon": "^3.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "@vitest/coverage-c8": "^0.30.1",
    "c8": "^7.13.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.7",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-progress": "^0.0.7",
    "vitest": "^0.30.1"
  }
}